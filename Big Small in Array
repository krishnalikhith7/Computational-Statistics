a = []
n = int(input("Enter size of array : "))
for x in range(n):
    x=int(input("enter element of array : "))
    a.append(x)
largest = a[0]
smallest = a[0]
for i in range(n):
    if a[i]>largest:
        largest = a[i]
    if a[i]<smallest:
        smallest = a[i]
        
print(f"Largest element in array is {largest}")
print(f"Smallest element in array is {smallest}") 
